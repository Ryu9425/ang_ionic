{
  "name": "@ionic/angular",
  "version": "6.1.6",
  "description": "Angular specific wrappers for @ionic/core",
  "keywords": [
    "ionic",
    "framework",
    "angular",
    "mobile",
    "app",
    "webapp",
    "capacitor",
    "cordova",
    "progressive web app",
    "pwa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/ionic/issues"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run build.ng && npm run build.core && npm run clean-generated",
    "build.core": "node scripts/build-core.js",
    "build.link": "npm run build && node scripts/link-copy.js",
    "build.ng": "ng-packagr -p ng-package.json -c tsconfig.json",
    "build.ng2": "ng build",
    "clean": "node scripts/clean.js",
    "clean-generated": "node ./scripts/clean-generated.js",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.ts\"",
    "eslint": "eslint . --ext .ts",
    "prerelease": "npm run validate && np prerelease --yolo --any-branch --tag next",
    "test": "echo 'angular no tests yet'",
    "tsc": "tsc -p .",
    "validate": "npm i && npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@ionic/core": "^6.1.6",
    "jsonc-parser": "^3.0.0",
    "tslib": "^2.1.0"
  },
  "peerDependencies": {
    "@angular/core": ">=12.0.0",
    "@angular/forms": ">=12.0.0",
    "@angular/router": ">=12.0.0",
    "rxjs": ">=6.6.0",
    "zone.js": ">=0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0-rc.2",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "^14.0.0-rc.2",
    "@angular/common": "^14.0.0-rc.2",
    "@angular/compiler": "^14.0.0-rc.2",
    "@angular/compiler-cli": "^14.0.0-rc.2",
    "@angular/core": "^14.0.0-rc.2",
    "@angular/forms": "^14.0.0-rc.2",
    "@angular/platform-browser": "^14.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^14.0.0-rc.2",
    "@angular/router": "^14.0.0-rc.2",
    "@ionic/eslint-config": "^0.3.0",
    "@ionic/prettier-config": "^2.0.0",
    "@schematics/angular": "^12.2.9",
    "@types/eslint": "^8.4.2",
    "@types/node": "12.12.5",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^7.0.0",
    "ng-packagr": "^14.0.0-rc.0",
    "prettier": "^2.4.1",
    "rxjs": "~6.6.0",
    "typescript": "~4.6.4",
    "zone.js": "~0.11.4"
  },
  "prettier": "@ionic/prettier-config",
  "schematics": "./schematics/collection.json"
}
